{
  "name": "content-moderation-app",
  "version": "1.0.0",
  "description": "A web application for content moderation",
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend start:uvicorn",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "start:uvicorn": "cd uvcorn && uvicorn app:app --host localhost --port 3334",
    "build": "cd frontend && npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}